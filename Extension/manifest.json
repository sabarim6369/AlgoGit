{
  "manifest_version": 3,
  "name": "AlgoGit",
  "version": "1.0",
  "description": "Sync your coding submissions across multiple platforms.",
  "permissions": ["storage", "scripting", "activeTab", "notifications","identity"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
       "16": "icons/AlgoGit.png",
    "32": "icons/AlgoGit.png",
    "48": "icons/AlgoGit.png",
    "128": "icons/AlgoGit.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["content-scripts/leetcode.js"],
      "run_at": "document_idle"
    },
     {
    "matches": ["https://practice.geeksforgeeks.org/problems/*",  "https://www.geeksforgeeks.org/problems/*"],
    "js": ["content-scripts/gfg.js"],
    "run_at": "document_idle"
  }
  ],
   "host_permissions": [
    "https://github.com/*"
  ],
    "oauth2": {
    "client_id": "Ov23liMfOlf2emj68SBT",
    "scopes": ["repo"]
  }
}
