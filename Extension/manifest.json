{
  "manifest_version": 3,
  "name": "AlgoGit",
  "version": "1.0",
  "description": "Sync your coding submissions across multiple platforms.",
  "permissions": ["storage", "scripting", "activeTab", "notifications","identity"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon1.png",
      "48": "icons/icon2.png",
      "128": "icons/icon2.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["content-scripts/leetcode.js"],
      "run_at": "document_idle"
    }
  ],
   "host_permissions": [
    "https://github.com/*"
  ],
    "oauth2": {
    "client_id": "Ov23liMfOlf2emj68SBT",
    "scopes": ["repo"]
  }
}
